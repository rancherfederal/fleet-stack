kind: GitRepo
apiVersion: fleet.cattle.io/v1alpha1
metadata:
  name: app-platform
  namespace: fleet-default
spec:
  repo: https://github.com/rancherfederal/fleet-stack.git
  branch: main

  targets:
  - clusterSelector:
      matchLabels:
        environment: dev

  paths:
  - "./gitops/apps/app_platform/"